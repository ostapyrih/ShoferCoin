<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="ShoferCoin" location="startMenu"/>
	<shortcut
		name="ShoferCoin Server"
		target="$INSTALL_PATH\ShoferCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start ShoferCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="ShoferCoin Wallet"
		target="http://localhost:6876/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch ShoferCoin wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>